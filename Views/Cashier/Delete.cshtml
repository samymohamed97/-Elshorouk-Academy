@model Cashier
@{
    ViewData["Title"] = "Delete";
}
    <form method="post" asp-action="DeletePost">
        <input asp-for="Id" hidden />
        <div class="border p-3 mt-4">
            <div class="row pb-2">
                <h2 class="text-primary">Delete Cashier</h2>
            </div>
            <div class="mb-3">
                <label asp-for="CashierName"></label>
                <input asp-for="CashierName" disabled class="form-control" />
            </div>
            <div class="mb-3">
                <label asp-for="BranchId"></label>
                <input asp-for="BranchId" disabled class="form-control" />
            </div>
            <button type="submit" class="btn btn-danger"> Delete</button>
            <a asp-controller="Cashier" asp-action="Index" class="btn btn-secondary">
                Back To List
            </a>
        </div>

    </form>